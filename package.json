{
  "name": "order-execution-engine",
  "version": "1.0.0",
  "description": "DEX order execution engine with WebSocket status updates",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --coverage --forceExit",
    "test:watch": "jest --watch --forceExit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "solana",
    "dex",
    "order-execution",
    "websocket",
    "fastify"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^10.0.1",
    "bullmq": "^5.13.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "postgresql": "^0.0.1",
    "redis-server": "^1.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
